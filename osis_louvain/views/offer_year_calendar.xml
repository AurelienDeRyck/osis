<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <!-- Tree -->
    <record id="offer_year_calendar_tree_view" model="ir.ui.view">
      <field name="name">offer_year_calendar.tree.view</field>
      <field name="model">osis.offer_year_calendar</field>
      <field eval="16" name="priority"/>
      <field name="arch" type="xml">
        <tree>
          <field name="event"/>
          <field name="start_date"/>
          <field name="end_date"/>
          <field name="academic_calendar_id"/>
          <field name="offer_year_id"/>

        </tree>
      </field>

    </record>
    <!-- Form -->
    <record id="offer_year_calendar_form" model="ir.ui.view">
      <field name="name">osis.view.offer_year_calendar.form</field>
      <field name="model">osis.offer_year_calendar</field>

      <field name="arch" type="xml">
        <form string="Offer Year Calendar Form">
          <group>
            <field name="event"/>
            <field name="start_date"/>
            <field name="end_date"/>
            <field name="academic_calendar_id"/>
            <field name="offer_year_id"/>
          </group>
        </form>
      </field>
    </record>

  </data>
</openerp>
