<openerp>
    <data>
      <report
            id="report_student"
            model="osis.student"
            string="Student Report pdf"
            name="osis_louvain.report_info_view"
            file="osis_louvain.report_student"
            report_type="qweb-pdf" />


            <template id="report_info_view">
              <t t-call="report.html_container">


           <t t-call="report.external_layout">
               <div class="page">
                   <h2>Report title</h2>
                   <t t-foreach="docs" t-as="o">
                   <p>This object's name is <span t-field="o.registration_number"/></p>
                    </t>
               </div>
           </t>

              </t>
            </template>

    </data>
</openerp>
