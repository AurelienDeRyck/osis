<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <!-- Tree -->
    <record id="tutor_tree_view" model="ir.ui.view">
      <field name="name">tutor.tree.view</field>
      <field name="model">osis.tutor</field>
      <field eval="16" name="priority"/>
      <field name="arch" type="xml">
        <tree>
          <field name="person_id" string="Name"/>
        </tree>
      </field>

    </record>
    <!-- Form -->
    <record id="osis_tutor_form" model="ir.ui.view">
      <field name="name">osis.view.tutorform</field>
      <field name="model">osis.tutor</field>
      <field name="arch" type="xml">
        <form string="Tutor simple Form">
          <group col="4">
            <field name="person_id"/>
          </group>
        </form>
      </field>

    </record>

  </data>
</openerp>
