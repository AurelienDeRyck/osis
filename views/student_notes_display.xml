<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>

    <!-- Tree -->
    <record id="student_notes_display_tree_view" model="ir.ui.view">
      <field name="name">student_notes_display.tree.view</field>
      <field name="model">osis.student_notes_display</field>
      <field eval="16" name="priority"/>
      <field name="arch" type="xml">

        <tree colors="green:status=='COMPLETE';red:status=='MISSING';orange:status=='PARTIAL'">

          <field name="status"/>
          <field name="title_offer"/>
          <field name="title_learning_unit"/>
          <field name="year"/>
          <field name="session_name"/>
          <field name="student_name"/>
        </tree>
      </field>
    </record>
    <!-- Form -->
    <record id="student_notes_display_form" model="ir.ui.view">
      <field name="name">osis.view.student_notes_display.form</field>
      <field name="model">osis.student_notes_display</field>

      <field name="arch" type="xml">
        <form string="Student Notes Display Form">
          <group col="4">
            <field name="title_offer"/>

          </group>
        </form>
      </field>
    </record>
  </data>
</openerp>
