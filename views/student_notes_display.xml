<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>

    <!-- Tree -->
    <record id="student_notes_display_tree_view" model="ir.ui.view">
      <field name="name">student_notes_display.tree.view</field>
      <field name="model">osis.student_notes_display</field>
      <field eval="16" name="priority"/>
      <field name="arch" type="xml">
        <tree colors="green:status=='COMPLETE';red:status=='MISSING';orange:status=='PARTIAL'" create="false" delete="false" editable="false">
          <field name="status"/>
          <field name="title_offer"/>
          <field name="title_learning_unit"/>
          <field name="year"/>
          <field name="session_name"/>
          <field name="student_name"/>
          <field name="score"/>
        </tree>
      </field>
    </record>
  </data>
</openerp>
